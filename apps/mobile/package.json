{
  "name": "@tunno/mobile",
  "private": true,
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client --clear",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "build": "exit 0",
    "build:android:development": "eas build --platform android --profile development",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:production": "eas build --platform android --profile production",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "check-deps": "expo install --check",
    "fix-deps": "expo install --fix",
    "eas-build-post-install": "turbo run build --filter='@repo/*'",
    "typecheck": "tsc --noEmit",
    "version:patch": "tsx ../../scripts/bump-version.ts patch",
    "version:minor": "tsx ../../scripts/bump-version.ts minor",
    "version:major": "tsx ../../scripts/bump-version.ts major"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-track-player"
        ]
      }
    }
  },
  "dependencies": {
    "@expo/config": "^12.0.10",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@hookform/resolvers": "^5.2.2",
    "@miblanchard/react-native-slider": "^2.6.0",
    "@react-navigation/native": "^7.1.21",
    "@repo/api": "workspace:*",
    "@repo/database": "workspace:*",
    "@repo/i18n": "workspace:*",
    "@repo/schemas": "workspace:*",
    "@repo/shared": "workspace:*",
    "@repo/utils": "workspace:*",
    "@shopify/flash-list": "^2.0.2",
    "@tanstack/react-query": "^5.90.7",
    "babel-plugin-inline-import": "^3.0.0",
    "culori": "^4.0.2",
    "drizzle-orm": "^0.44.7",
    "expo": "^54.0.25",
    "expo-constants": "~18.0.8",
    "expo-dev-client": "~6.0.11",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.19",
    "expo-font": "~14.0.9",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.9",
    "expo-router": "~6.0.15",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.11",
    "expo-sqlite": "~16.0.9",
    "expo-system-ui": "~6.0.8",
    "expo-updates": "~29.0.13",
    "lodash": "^4.17.21",
    "lottie-react-native": "~7.3.4",
    "lucide-react-native": "^0.553.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.66.0",
    "react-native": "0.81.5",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "^1.18.5",
    "react-native-mmkv": "^4.0.1",
    "react-native-reanimated": "~4.1.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-track-player": "5.0.0-alpha0",
    "react-native-worklets": "^0.5.1",
    "sonner-native": "^0.21.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@types/culori": "^4.0.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.6.0",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.10",
    "drizzle-kit": "^0.31.6",
    "typescript": "~5.9.3"
  }
}
