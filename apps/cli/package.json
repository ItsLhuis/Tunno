{
  "name": "@tunno/cli",
  "version": "1.6.1",
  "description": "Command-line tool for managing your personal music library",
  "main": "dist/bin/tunno.js",
  "bin": {
    "tunno": "dist/bin/tunno.js"
  },
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/bin/tunno.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "version:patch": "tsx ../../scripts/bump-version.ts patch",
    "version:minor": "tsx ../../scripts/bump-version.ts minor",
    "version:major": "tsx ../../scripts/bump-version.ts major"
  },
  "keywords": [
    "cli",
    "music",
    "youtube",
    "spotify",
    "downloader",
    "audio",
    "media"
  ],
  "author": "ItsLhuis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsLhuis/Tunno.git",
    "directory": "apps/cli"
  },
  "homepage": "https://github.com/ItsLhuis/Tunno/tree/main/apps/cli",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@spotify/web-api-ts-sdk": "^1.2.0",
    "archiver": "^7.0.1",
    "axios": "^1.7.9",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "inquirer": "^12.2.0",
    "natural": "^8.0.1",
    "ora": "^9.0.0",
    "sharp": "^0.33.5",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/node": "^22.10.2",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist"
  ]
}
